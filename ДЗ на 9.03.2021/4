#include<iostream>
using namespace std;


template <typename X> X distance(X x, X y)
{
	return sqrt(x*x + y * y);
}

template <typename Y> Y distance(Y x, Y y, Y z)
{
	return sqrt(x * x + y * y + z * z);
}
template <typename Z>
struct point {
	Z x, y;
};

template <typename Q>
struct point {
	Q x, y, z;
};



int main() {
	
	return 0;
}
